<Application x:Class="JobOverview.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:JobOverview"
			 xmlns:vw="clr-namespace:JobOverview.View"
		     xmlns:vm="clr-namespace:JobOverview.ViewModel"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:VMLogin}">
            <vw:UCLogin/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:VMEchange}">
            <vw:UCEchange/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:VMTachesAnnexes}">
            <vw:UCTachesAnnexes/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:VMSaisieTemps}">
            <vw:UCSaisieTemps/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:VMProgressBar}">
            <vw:UCProgressBar/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:VMTachesProd}">
            <vw:UCTachesProd/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:VMSaisieTache}">
            <vw:UCSaisieTaches/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:VMSyntheseVersion}">
            <vw:UCSyntheseVersion/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:VMAPropos}">
            <vw:UCAPropos/>
        </DataTemplate>
    </Application.Resources>
</Application>
